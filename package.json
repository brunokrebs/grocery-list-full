{
  "name": "klan",
  "version": "0.0.1",
  "description": "Koa, LokiJS, Angular 2 and Node application",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "build-server": "babel -d ./dist/server ./src/server -s",
    "build-client": "webpack --config config/webpack.dev.babel.js --progress",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "dev": "nodemon --exec babel-node -- src/server/app.js",
    "start": "node dist/server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "koa",
    "lokijs",
    "angular2",
    "node",
    "javascript"
  ],
  "author": "Bruno Krebs",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^6.0.45",
    "@types/jasmine": "^2.5.35",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-cli": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "koa": "^1.2.4",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "typescript": "2.1.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "core-js": "^2.4.1",
    "koa-static": "^2.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  }
}
